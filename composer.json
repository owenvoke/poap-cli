{
    "name": "owenvoke/poap-cli",
    "description": "A PHP CLI for managing your POAPs",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.5",
        "http-interop/http-factory-guzzle": "^1.2",
        "illuminate/http": "^9.49",
        "illuminate/view": "^9.49",
        "laravel-zero/framework": "^9.2",
        "nunomaduro/termwind": "^1.15",
        "owenvoke/poap": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/poap"]
}
